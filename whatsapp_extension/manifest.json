{
    "name": "WhatsAPp",
    "version": "0.0.0.1",
    "description": "Build an Extension!",
    "permissions":["tabs"],
    "content_scripts":[
        {
            "matches":[
                    "<all_urls>"
            ],
            "js":["p5.js","p5.dom.js","p5.sound.js","jqery.js","jqery.core.js","content.js"]
        }
    ],
    "web_accessible_resources":["whatsapp/b.png"],
  "browser_action":
  {
      "default_popup":"whatsapp/pop.html",
      "default_title":"Whatsapp Snippet"
  },
  "options_page": "feedback_option/options.html",
    "manifest_version": 2
  }
